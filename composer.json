{
    "name": "stanvk/magister",
    "description": "An eloquent API for Schoolmaster BV's Magister 6",
    "license": "MIT",
    "authors": [
        {
            "name": "Stan van Kraaij"
        },
        {
            "name": "Christopher"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.1",
        "phpunit/phpunit": "~4.0"
    },
    "suggest": {
        "symfony/var-dumper": "Required to use the pretty dump functionality (^2.7).",
        "filp/whoops": "Required to use the pretty error interface for easy debugging (1.1.*)."
    },
    "autoload": {
        "files": [
            "src/Magister/Services/Support/helpers.php"
        ],
        "psr-4": {
            "Magister\\": "src/Magister"
        }
    },
    "minimum-stability": "stable"
}
